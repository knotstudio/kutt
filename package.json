{
  "private": true,
  "name": "@knotstudio/kutt",
  "version": "2.3.14",
  "description": "Modern URL shortener.",
  "workspaces": ["apps/*"],
  "main": "./server/dist/server.js",
  "scripts": {
    "start:backend": "./support/dev-backend",
    "start:frontend": "./support/dev-frontend",
    "lint": "eslint apps/* --ext .js,.ts",
    "lint:fix": "eslint apps/* --ext .js,.ts --fix"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn run lint"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/knotstudio/kutt.git"
  },
  "keywords": [
    "url-shortener"
  ],
  "author": "Lochlan Bunn <bunn@lochlan.io>",
  "contributors": [
    "Pouria Ezzati <ezzati.upt@gmail.com>"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/knotstudio/kutt/issues"
  },
  "homepage": "https://github.com/knotstudio/kutt#readme",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^2.16.0",
    "@typescript-eslint/parser": "^2.16.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.18.0",
    "husky": "^0.15.0-rc.13",
    "prettier": "^1.19.1",
    "redoc": "^2.0.0-rc.20",
    "typescript": "^3.7.5"
  }
}
